{
  "name": "node-rest-api",
  "version": "0.0.1",
  "description": "Configurable node rest API",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config tools/webpack.config.js",
    "app:build": "npm run build -- --mode production",
    "dev:build": "npm run build -- --mode development",
    "dev:server": "nodemon --watch dist --watch server.js --delay 2.5 server.js",
    "start": "run-p dev:*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nishantsinha-5/node-rest-api.git"
  },
  "keywords": [
    "node rest",
    "rest-api",
    "express rest wrapper",
    "configuration only rest api"
  ],
  "author": "Nishant Kumar Sinha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nishantsinha-5/node-rest-api/issues"
  },
  "homepage": "https://github.com/nishantsinha-5/node-rest-api#readme",
  "dependencies": {
    "chalk": "2.4.1",
    "errorhandler": "1.5.0",
    "express": "4.16.3"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-preset-env": "1.7.0",
    "eslint": "5.4.0",
    "eslint-loader": "2.1.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "4.1.3",
    "source-map-support": "0.5.9",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-node-externals": "1.7.2"
  },
  "engines": {
    "node": ">=6.9.1"
  }
}
