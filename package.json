{
  "name": "node-rest-server",
  "version": "1.0.1",
  "description": "Configurable node rest server",
  "main": "lib/index.js",
  "scripts": {
    "prod:build": "webpack --config configuration/webpack.prod.js",
    "dev:build": "webpack --config configuration/webpack.dev.js",
    "dev:server": "nodemon --watch lib --watch server.js --delay 2.5 server.js",
    "start": "run-p dev:*",
    "format": "prettier './src/**/*' --write",
    "precommit": "npm run format"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nishant-labs/node-rest-server.git"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "node rest",
    "rest-server",
    "express rest wrapper",
    "configuration only rest server"
  ],
  "author": "Nishant Kumar Sinha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nishant-labs/node-rest-server/issues"
  },
  "homepage": "https://github.com/nishant-labs/node-rest-server#readme",
  "dependencies": {
    "chalk": "2.4.1",
    "date-fns": "1.29.0",
    "errorhandler": "1.5.0",
    "express": "4.16.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-classes": "7.1.0",
    "@babel/plugin-transform-template-literals": "7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "^8.0.2",
    "eslint": "5.6.0",
    "eslint-loader": "2.1.1",
    "nodemon": "^1.18.4",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.3",
    "source-map-support": "0.5.9",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0",
    "webpack-merge": "4.1.4",
    "webpack-node-externals": "1.7.2"
  }
}
